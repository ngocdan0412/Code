<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="400.0"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
	<children>
		<Button fx:id="login" layoutX="60.0" layoutY="245.0"
			mnemonicParsing="false" onAction="#login" prefHeight="30.0"
			prefWidth="90.0" text="LOGIN" />
		<Button fx:id="exit" layoutX="243.0" layoutY="245.0"
			mnemonicParsing="false" onAction="#exit" prefHeight="30.0"
			prefWidth="90.0" text="EXIT" />
		<TextField fx:id="input2" layoutX="146.0" layoutY="185.0"
			onAction="#enterPassword" />
		<TextField fx:id="input1" layoutX="146.0" layoutY="135.0"
			onAction="#enterUsername" />
		<Text layoutX="49.0" layoutY="156.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="USERNAME" />
		<Text layoutX="48.0" layoutY="206.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="PASSWORD" />
		<Text layoutX="134.0" layoutY="97.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="LOGIN">
			<font>
				<Font size="45.0" />
			</font>
		</Text>
	</children>
</AnchorPane>
